pipeline{
    agent(node-1){


    }
    stages{
        stage('build'){
            steps{
                echo "building jenkins file 14/1/26"
            }
        }
        stage('test'){
            steps{
                echo "testing"
            }
        }
        stage('deploy'){
            steps{
                echo "deploying"
            }
        }
    }
    post{
        always{
            cleanWs()
        }
    }

}

 